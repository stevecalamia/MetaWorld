#eye {
  height: 200px;
  width: 400px;
  position: absolute;
  top: 40%;
  left:40%;
  background-color: #e7c388;
  -moz-border-radius: 125px;
  /*mask: url("/assets/eye-mask.svg");*/  
}

.corner {
  position: absolute;
}
.corner.container {
  -moz-transform:rotate(344deg);
  height:71px;
  left:-4px;
  overflow:hidden;
  top:54px;
  width:42px;
}

.corner.triangle {
  -moz-box-shadow:1px -1px 8px #000000 inset;
  -moz-transform:rotate(56deg);
  background-image:-moz-radial-gradient(60% 40% 0deg, circle farthest-side, transparent, transparent 85%, #FF0000 94%, #F27575);
  border-left:1px solid black;
  height:65px;
  left:20px;
  top:26px;
  width:68px;
}

.corner.box {
  -moz-border-radius:10px 10px 10px 10px;
  -moz-box-shadow:1px -1px 3px #3C3C3C inset;
  -moz-transform:rotate(45deg);
  background-image:-moz-radial-gradient(30% 29% 0deg, ellipse farthest-corner, #FFFFFF, #FFDBAB 10%, #FFABAB 19%, #FF0000);
  border-bottom:1px solid black;
  border-left:1px solid black;
  height:23px;
  left:4px;
  top:44px;
  width:17px;
}

#top-half, #bottom-half {height:100px;
overflow:hidden;}

#top-half {
  
}

#bottom-half {
  
}

.top-white, .bottom-white {
  background-image:-moz-radial-gradient(50% 50% 0deg, ellipse closest-side, #FFFFDD, #FFFFDD 83%, #DDDCC3 93%, #7C7C6D 98%, transparent 98%, transparent);
height:226px;
width:400px;
}

.top-white {
background-position:0 21px;
}

.bottom-white {

background-position:0 -146px;
background-repeat:no-repeat;
}

.iris, .glare.main {height:150px;
left:122px;
position:absolute;
top:25px;
width:150px;
}

.iris {-moz-border-radius:75px 75px 75px 75px;
-moz-transform:rotate(300deg);
background:-moz-radial-gradient(50% 50% 0deg, circle farthest-corner, #000000, #000000 26%, #0000FF 44%, #00259B 71%, transparent 90%, transparent) no-repeat scroll 0 0 transparent;
}

.glare.main {
  background-image: -moz-radial-gradient(28% 27% 0deg, circle farthest-corner, rgba(255,255,255,.9), rgba(255,255,255,.9) 5%, rgba(255,255,255,0) 20%, transparent);
  z-index: 25;
}

.iris-shading { 
  height: 75px; 
  width: 25px; 
  position: absolute; 
  background-image: -moz-radial-gradient(50% 100% 0deg, ellipse farthest-side, rgba(127,127,255, 0), rgba(127,127,255,0) 50%, rgba(127,127,127,.5) 60%, rgba(127,127,127,0) 70%, rgba(127,127,127,.25) 80%, rgba(127,127,127,0) 90%, rgba(127,127,127,0));
}

.shading {
  height: 75px;
  width: 75px;
  position: absolute;
  background-image: -moz-radial-gradient(50% 0% 0deg, ellipse farthest-side, rgba(127,127,255, 0), rgba(127,127,255,0) 10%, rgba(127,127,127,.5) 20%, rgba(127,127,127,0) 30%, rgba(127,127,127,.35) 40%, rgba(127,127,127,0) 50%, rgba(127,127,127,0));
  -moz-transform-origin: 50% 100%;
  top: 1px;
  left: 37px;
}

